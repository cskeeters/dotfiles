[user]
  name = Chad Skeeters
  email = chad.skeeters@gmail.com
[credential]
  helper = osxkeychain
[alias]
  st = status
  sta = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  ds = diff --staged
  lg = log -p
  gr = log --graph --full-history --all --color --pretty=format:"%x1b[31m%h%x09%x1b[32m%d%x1b[0m%x20%s"
  l = log --graph --oneline
[push]
	default = simple
