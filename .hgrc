[ui]
username=goobsoft@gmail.com

[extensions]
hgext.mq=
hgk=
graphlog=
color=

[hgk]
#path=/usr/local/bin/hgk
path=/opt/local/share/mercurial/contrib/hgk

[diff]
git=1

[defaults]
revert=--no-backup

[alias]
slog = log --style=/home/chad/mercurial-cli-templates/map-cmdline.slog
show = log -pr
nlog = log --style=/home/chad/mercurial-cli-templates/map-cmdline.nlog
sglog = glog --style=/home/chad/mercurial-cli-templates/map-cmdline.sglog
, = sglog -l15
