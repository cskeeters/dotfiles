  sta = status
  ci = commit
  br = branch
  co = checkout
  d  = diff
  df = diff
  ds = diff --staged
  l  = log --graph --pretty=format:'%C(red)%h%C(reset) %C(yellow)%d%C(reset)%s %Cgreen(%cr) %C(bold blue)<%an>%C(reset)' --abbrev-commit --date=relative
  la = log --graph --pretty=format:'%C(red)%h%C(reset) %C(yellow)%d%C(reset)%s %Cgreen(%cr) %C(bold blue)<%an>%C(reset)' --abbrev-commit --date=relative --all
  gl = log --graph --pretty=format:'%C(red)%h%C(reset) %C(yellow)%d%C(reset)%s %Cgreen(%cr) %C(bold blue)<%an>%C(reset)' --abbrev-commit --date=relative --all
  ll = log --graph --pretty=format:'%C(red)%h%C(reset) %C(yellow)%d%C(reset)%s %Cgreen(%cr) %C(bold blue)<%an>%C(reset)' --abbrev-commit --date=relative --max-count=20
[merge]
  tool = tabsplice
[mergetool "tabsplice"]
  cmd = tabsplice $BASE $LOCAL $REMOTE $MERGED
  trustExitCode = true
[mergetool]
  keepBackup = false
[pull]
